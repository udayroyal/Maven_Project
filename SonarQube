

pipeline {
		agent any
		stages {
				
							stage ( 'Compile Stage' ) {
								steps {
									withMaven (maven : 'MAVEN_3' ) {
										sh 'mvn clean compile package'
												}
											}
										}
							stage ( 'Code Coverage' ) {
								steps {
											jacoco()
												}
											}
					}							

				
			}